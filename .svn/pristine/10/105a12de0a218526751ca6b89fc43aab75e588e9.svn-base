<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.ddit.kixiv.tag.mapper.TagDao">
	<select id="list" resultType="kr.ddit.kixiv.tag.vo.TagVo">
		select * from tag
	</select>
	
	<select id="tagList" resultType="kr.ddit.kixiv.tag.vo.TagVo">
		select * from tag 
		where tag_id = #{tag_id}
	</select>
	
</mapper>