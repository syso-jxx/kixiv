package kr.ddit.kixiv.gallery.vo;

import java.util.Date;

public class GalleryRentVO {
	private String rent_num;
	private String cancel_yn;
	private String rent_tel;
	private int rent_seq;
	private String user_id;
	private Date rent_req_date;
	private String rent_name;
	private String rent_email1;
	private String rent_email2;
	private String rent_imp_uid; // : imp_uid 고유번호(환불 요청시 고유번호로 사용)
	private String rent_pg;
	private String name;
	private String rent_book;
	
	public GalleryRentVO() {	}
	
	public String getRent_book() {
		return rent_book;
	}

	public void setRent_book(String rent_book) {
		this.rent_book = rent_book;
	}

	public String getRent_num() {
		return rent_num;
	}
	
	public void setRent_num(String rent_num) {
		this.rent_num = rent_num;
	}

	public String getRent_email1() {
		return rent_email1;
	}

	public void setRent_email1(String rent_email1) {
		this.rent_email1 = rent_email1;
	}

	public String getRent_email2() {
		return rent_email2;
	}

	public void setRent_email2(String rent_email2) {
		this.rent_email2 = rent_email2;
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	public String getRent_pg() {
		return rent_pg;
	}

	public void setRent_pg(String rent_pg) {
		this.rent_pg = rent_pg;
	}

	public String getRent_imp_uid() {
		return rent_imp_uid;
	}

	public void setRent_imp_uid(String rent_imp_uid) {
		this.rent_imp_uid = rent_imp_uid;
	}
	private int rent_price; 
	

    public int getRent_price() {
		return rent_price;
	}

	public void setRent_price(int rent_price) {
		this.rent_price = rent_price;
	}
	public String getCancel_yn() {
		return cancel_yn;
	}
	public void setCancel_yn(String cancel_yn) {
		this.cancel_yn = cancel_yn;
	}
	public String getRent_tel() {
		return rent_tel;
	}
	public void setRent_tel(String rent_tel) {
		this.rent_tel = rent_tel;
	}
	public int getRent_seq() {
		return rent_seq;
	}
	public void setRent_seq(int rent_seq) {
		this.rent_seq = rent_seq;
	}
	public String getUser_id() {
		return user_id;
	}
	public void setUser_id(String user_id) {
		this.user_id = user_id;
	}
	public Date getRent_req_date() {
		return rent_req_date;
	}
	public void setRent_req_date(Date rent_req_date) {
		this.rent_req_date = rent_req_date;
	}
	public String getRent_name() {
		return rent_name;
	}
	public void setRent_name(String rent_name) {
		this.rent_name = rent_name;
	}
	@Override
	public String toString() {
		return "GalleryRentVO [rent_num=" + rent_num + ", cancel_yn=" + cancel_yn + ", rent_tel=" + rent_tel
				+ ", rent_seq=" + rent_seq + ", user_id=" + user_id + ", rent_req_date=" + rent_req_date
				+ ", rent_name=" + rent_name + ", rent_email1=" + rent_email1 + ", rent_email2=" + rent_email2
				+ ", rent_imp_uid=" + rent_imp_uid + ", rent_pg=" + rent_pg + ", name=" + name + ", rent_book="
				+ rent_book + ", rent_price=" + rent_price + "]";
	}
	
}
